{
    "name": "piklist/piklist",
    "description": "The most powerful framework available for WordPress.",
    "type": "wordpress-plugin",
    "require": {
        "phpdocumentor/phpdocumentor": "2.8.x",
        "cvuorinen/phpdoc-markdown-public": "^0.1.2"
    },
    "scripts": {
      "build-docs": "./vendor/bin/phpdoc --directory=includes --target=docs/ --template=\"vendor/cvuorinen/phpdoc-markdown-public/data/templates/markdown-public\" --title=\"Piklist Documentation\""
    }
}
